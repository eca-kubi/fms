<service>
	<id>pdfdownloadserver.exe</id>
	<name>PDF Download Server</name>
	<description>This service starts a node js server running of port 3000, which handles PDF downloading for clients</description>
	<executable>C:\Program Files\nodejs\node.exe</executable>
	<argument>--harmony</argument>
	<argument>C:\Users\IT\AppData\Roaming\npm\node_modules\node-windows\lib\wrapper.js</argument>
	<argument>--file</argument>
	<argument>C:\xampp\htdocs\fms\app.js</argument>
	<argument>--scriptoptions=</argument>
	<argument>--log</argument>
	<argument>PDF Download Server wrapper</argument>
	<argument>--grow</argument>
	<argument>0.25</argument>
	<argument>--wait</argument>
	<argument>1</argument>
	<argument>--maxrestarts</argument>
	<argument>3</argument>
	<argument>--abortonerror</argument>
	<argument>n</argument>
	<argument>--stopparentfirst</argument>
	<argument>undefined</argument>
	<logmode>rotate</logmode>
	<stoptimeout>30sec</stoptimeout>
	<serviceaccount>
		<domain>NZELT-ITD-03</domain>
		<user>LocalSystem</user>
		<password></password>
	</serviceaccount>
	<workingdirectory>C:\xampp\htdocs\fms</workingdirectory>
</service>